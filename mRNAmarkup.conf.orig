# mRNAmarkup.conf
#
# Last update: April 8, 2015.  Volker Brendel.
#

# Locations and names of protein databases:
#
VectorDBdir=INSTALLDIR/db
VectorDB=UniVec
BacteriaDBdir=INSTALLDIR/db
BacteriaDB=Ecoli
ReferenceDBdir=INSTALLDIR/db
ReferenceDB=ATpepTAIR10
AllProteinDBdir=INSTALLDIR/db
AllProteinDB=UniRef90v
HypotheticalProteinDBdir=INSTALLDIR/db
HypotheticalProteinDB=HXpep
ProteinDomainDBdir=INSTALLDIR/db
ProteinDomainDB=Cdd
miRBasedir=INSTALLDIR/db
miRBase=miRBase19

# BLAST+ parameters:
#
numThreads=1
tmpdir=/tmp
revalue=1e-20
RnumDescriptions=10
RnumAlignments=10
aevalue=1e-20
AnumDescriptions=10
AnumAlignments=5
hevalue=1e-20
HnumDescriptions=10
HnumAlignments=5
devalue=1e-10
DnumDescriptions=10
DnumAlignments=5

# MuSeqBox column width parameters:
#
idsize=40
dbsize=5
ansize=80
srsize=24

# MuSeqBox parameters for establishing potential full-length coding
# sequences:
#
Rnhits=1
Rnhsps=1
Rv5s=5
Rv3s=5
Rv5q=10
Rv3q=10
Rscv=90
Rqcv=60

# MuSeqBox parameters for annotation:
#
pid=45
mao=10
scv=75

# ESTScan parameters:
#
trainESTScan=1
